{
  "version": "1.0.1",
  "license": "MIT",
  "repository": "https://github.com/luc-freyermuth/rxjs-universal-combine-latest",
  "keywords": [
    "rxjs",
    "generator",
    "operator",
    "combineLatest",
    "universal",
    "deep",
    "object",
    "utility"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "rxjs": "^6.6.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "name": "rxjs-universal-combine-latest",
  "author": "Luc Freyermuth",
  "module": "dist/rxjs-universal-combine-latest.esm.js",
  "size-limit": [
    {
      "path": "dist/rxjs-universal-combine-latest.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rxjs-universal-combine-latest.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.1",
    "dts-cli": "^0.19.2",
    "husky": "^7.0.2",
    "rxjs": "^6.6.7",
    "size-limit": "^6.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
